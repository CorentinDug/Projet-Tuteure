var tab = [];
$(document).ready(function(){

    /* Appelle le php qui contient le tableau php encodée en json */

    $.getJSON('http://localhost/projet/Projet-Tuteure/web/app_dev.php/menu/add', function(data) {
        /* [XMLmind] contiendra le tableau php en tant qu’objet javascript */

        $.each(data, function(key, val) {

            tab[key] = val;
            console.log(tab[key])

        });

    });
    $( "#plat" ).autocomplete({
        source: tab
    });
});
